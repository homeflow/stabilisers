<li class="span8">
  <div class="thumbnail row">
    <a href="{{property | url_for_property}}">
      <img class='span4' src="{{ property.photos.first | url_for_property_asset : '260x180'}}" alt="">
    </a>
    <div class='span3'>
      <h4>{{property.display_address}}</h4>
      <p>{{property.price}}</p>
      <p>{{property.short_description | truncate : 150}}</p>
    </div>
  </div>
</li>
